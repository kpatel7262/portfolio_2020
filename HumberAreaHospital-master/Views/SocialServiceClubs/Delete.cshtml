@model HumberAreaHospitalProject.Models.SocialServiceClubs
@{
    ViewBag.Title = "Delete";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title></title>
</head>
<body>
    <div>
        <h2>Are you sure you want to delete this Social Service Club?</h2>
        <button class="btn btn-sm">
            <a href="/SocialServiceClubs/List">No</a>
        </button>
        <div class="form-group"><h3>Title:</h3></div>
        <div><p class="form-group"><h3>@Model.SocialServiceClubs_title</h3></div>

        <div class="form-group">
            <form method="post" action="/SocialServiceClubs/Delete/@Model.SocialServiceClubs_id">
                <input class="btn btn-secondary" type="submit" value="Delete" />
            </form>
        </div>

    </div>
</body>
</html>