
@model  HumberAreaHospitalProject.Models.FAQCategory
@{
    ViewBag.Title = "Update";
}

<h2>Update Category</h2>
<form method="post" action="/FAQCategory/Update/@Model.FAQCategoryID">
    <div class="form-group">
        <label class="col-sm-2 col-form-lable">Category Name</label>
        <input type="text" class="form-control" name="FAQCategoryName" value="@Model.FAQCategoryName"/>
    </div>
    <div class="form-group">
        <input type="submit" value="UPDATE" />
    </div>
</form>